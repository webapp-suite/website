{
  "extends": [
    "standard",
    "standard-react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "__CLIENT__": true,
    "__SERVER__": true,
    "__DEVELOPMENT__": true,
    "__DEVTOOLS__": true,
    "runtime": true,
    "jsdom": true,
    "prefixCls": true
  },
  "parser": "babel-eslint",
  "rules": {
    "jsx-quotes": ["error", "prefer-double"],
    "no-unused-expressions": "off",
    "react/jsx-no-bind": "off",
    "react/no-did-update-set-state": "off"
  }
}
